    "babel-runtime": "^6.18.0",
    "babel-watch": "^2.0.7",
    "element-ui": "^1.4.1",
    "eslint-config-standard": "^10.2.1",
    "mocha": "^3.4.2",
    "promise-polyfill": "^6.0.2",
